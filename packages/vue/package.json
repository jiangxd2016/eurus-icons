{
  "name": "eurus-icons-vue",
  "type": "module",
  "version": "0.0.15",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangxd2016/eurus-icons",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/jiangxd2016/eurus-icons/issues"
  },
  "keywords": [
    "icon",
    "svg",
    "vue",
    "eurus-icons"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "import": "./dist/global.js",
      "require": "./dist/global.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:generate && pnpm run build:build && pnpm run build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^5.0.1",
    "@pnpm/find-workspace-packages": "^5.0.40",
    "@pnpm/logger": "^5.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "consola": "^2.15.3",
    "esbuild": "^0.17.11",
    "esbuild-plugin-globals": "^0.2.0",
    "eurus-icons-svg": "workspace:*",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "prettier": "^2.8.4",
    "tsx": "^3.12.5",
    "typescript": "^4.9.5",
    "unplugin-vue": "^4.0.2",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  }
}
